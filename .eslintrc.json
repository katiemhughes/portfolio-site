{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "overrides": [
    {
      "rules": {
        "react/react-in-jsx-scope": "off",
        "no-console": "off",
        "react/no-array-index-key": "off",
        "import/no-extraneous-dependencies": "off"
      },
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ]
  
}
